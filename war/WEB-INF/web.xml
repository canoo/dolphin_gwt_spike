<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
		 version="2.5">

	<servlet>
		<servlet-name>com.canoo.dolphingwtspike.mainApplication.MainApplication MainApplicationService</servlet-name>
		<servlet-class>com.canoo.dolphingwtspike.mainApplication.server.MainApplicationServiceImpl</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DolphinServlet</servlet-name>
		<servlet-class>com.canoo.dolphingwtspike.mainApplication.server.servlet.ApplicationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>com.canoo.dolphingwtspike.mainApplication.MainApplication MainApplicationService</servlet-name>
		<url-pattern>/MainApplication/MainApplicationService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DolphinServlet</servlet-name>
		<url-pattern>/dolphin</url-pattern>
	</servlet-mapping>
</web-app>
