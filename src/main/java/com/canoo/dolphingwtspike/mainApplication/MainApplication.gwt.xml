<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN" "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="MainApplication">

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>

	<!-- Specify the app entry point class.                   -->
	<entry-point class='com.canoo.dolphingwtspike.mainApplication.client.MainApplication'/>


	<!-- Specify the app servlets.                   -->
	<servlet path='/MainApplicationService' class='com.canoo.dolphingwtspike.mainApplication.server.MainApplicationServiceImpl'/>

</module>