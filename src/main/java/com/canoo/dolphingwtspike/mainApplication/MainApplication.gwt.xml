<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN" "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="MainApplication">

	<!-- Inherit the core Web Toolkit stuff: -->
	<inherits name='com.google.gwt.user.User'/>
	<inherits name="com.canoo.opendolphin.OpenDolphin"/>

	<!-- GWT Application entry point: -->
	<entry-point class='com.canoo.dolphingwtspike.mainApplication.client.MainApplication'/>

	<add-linker name="xsiframe"/>
	<!--<set-configuration-property name="devModeRedirectEnabled" value="true"/>-->
	<set-property name="compiler.useSourceMaps" value="true"/>

</module>